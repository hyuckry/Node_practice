{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch React App Sample",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/",
            "webRoot": "${workspaceFolder}/react-app-sample"
        },
        {
            "name": "Launch Express Server",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/react-app-sample",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "express"
            ],
            "port": 9229
        },
        {
            "name": "Launch Blank Page",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}/blank"
        }
    ]
}
